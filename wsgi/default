server {
   server_name chili;
   gzip off;
   listen 8000 default;

   location = /favicon.ico  {
       rewrite "/favicon.ico" /img/favicon.ico;
   }
   location /media/  {
       root /nfs/milo/u1/username/Code/chili;
   }
   location /tmp/  {
       root /nfs/milo/u1/username/Code/chili;
   }

   proxy_set_header Host $host;
   location / {
       proxy_pass http://127.0.0.1:8080/;
   }
}
